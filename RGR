<html>
 <head> 
 <script type="text/javascript" 
src="https://www.google.com/jsapi"></script> 
<script> google.load("visualization", "1.1", 
{packages:["wordtree"]});
 google.setOnLoadCallback(drawChart);
 function drawChart() {
 var data = google.visualization.arrayToDataTable(
 [ ['Диплом'],
 [''],
 ['Диплом що потрібно підготувати'],
 [''],
 ['Диплом що потрібно підготувати допуск'],
 ['Диплом що потрібно підготувати захисне слово'],
 ['Диплом що потрібно підготувати презентація'],
 [''],
 [' Диплом виступ'],
 [''],
 ['Диплом виступ привітання атестаційної коміссії'],
 ['Диплом виступ тема дипломного проєкту'],
 ['Диплом виступ коротке введення до теми'],
 ['Диплом виступ затребуваність професії'],
 ['Диплом виступ цілі та задачі '],
 ['Диплом виступ обєкти і субєкти дослідження'],
 ['Диплом виступ підведення підсумків та висновків'],
 ['Диплом виступ висновок та вдячність науковому керівнику'],
 [''],
 [' Диплом захист'],
 [''],
 ['Диплом захист роботу написано самостійно'],
 ['Диплом захист показати ціність та предметність знань'],
 ['Диплом захист відобразити розуміння спеціальності'],
 ['Диплом захист відчути готовність до професійної діяльності'],

 ]
 );
 var options = {
maxFontSize: 28,
 wordtree: {
 format: 'implicit',
 word: 'Диплом'
 }
 };
 var chart = new google.visualization.WordTree
(document.getElementById('wordtree_basic'));
 chart.draw(data, options);
 } 
 </script>
 </head>
 <body>
 <div id="wordtree_basic" style="width: 100%; 
height: 500px;"></div>
 
